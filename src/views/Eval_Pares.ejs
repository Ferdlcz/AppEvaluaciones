        <div class="container">
<table class="table" style="text-align: center; margin-top: 30px;">
  
    <%if(respar.length>0){%> 
        <tr>
            <th>Codigo</th>
            <th>Nombre Completo</th>
            <th>Direccion</th>
            <th>Calificacion </th>
        </tr>
    
        <%for(var x=0;x < respar.length;x++){%>
            <tr>
              <td><%= respar[x].codigo %></td>
              <td><%= respar[x].nombrecompleto %></td>
              <td><%= respar[x].direccion %></td>
              <td><%= respar[x].calificacion %></td>
            </tr>
            <%}%>
    </table>
    <%}%>
